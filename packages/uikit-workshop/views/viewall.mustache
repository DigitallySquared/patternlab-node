<!-- View All Patterns in a Pattern Sub-Type -->
<div class="pl-c-main">

	<!-- Index of Patterns -->
	<div class="pl-c-pattern-index">

		{{# partials }}
		{{# patternSectionSubGroup }}
		{{> patternSectionSubGroup }}
		{{/ patternSectionSubGroup }}
		{{^ patternSectionSubGroup }}
		{{> patternSection }}
		{{/ patternSectionSubGroup }}
		{{/ partials }}

	</div>
	<!--end pl-c-pattern-index-->

</div>
<!--end pl-c-main-->